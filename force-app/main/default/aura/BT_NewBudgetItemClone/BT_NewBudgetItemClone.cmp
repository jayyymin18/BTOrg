                                                                                <lightning:formattedNumber value="{!total.fieldValue}" style="currency" currencyDisplayAs="code" />
                                                                                <aura:set attribute="else">
                                                                                    <lightning:formattedNumber value="0" style="currency" currencyDisplayAs="code" />
                                                                                </aura:set>
                                                                            </aura:if>
                                                                        </aura:if>
                                                                    </td>
                                                                </aura:iteration>
                                                            </tr>
                                                            <!-- <br/> -->
                                                        </aura:iteration>

                                                    </aura:iteration>

                                                </aura:iteration>

                                            </aura:iteration>
                                        </aura:if>

                                    </tbody>
                                </table>
                            </div>
                            </aura:set>
                            </aura:if>

