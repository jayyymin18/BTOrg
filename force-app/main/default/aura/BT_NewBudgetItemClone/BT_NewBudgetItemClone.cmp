                                                                                        <aura:set attribute="else">
                                                                                            <lightning:formattedNumber value="0" style="percent" maximumFractionDigits="2" />
                                                                                        </aura:set>
                                                                                    </aura:if>
                                                                                </aura:if>

                                                                                <aura:set attribute="else">
